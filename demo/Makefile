# ============================================================================#
#                                                                             #
#                      Makefile for  YZONE/demo                               #
#                                                                             #
# ============================================================================#

YDIR_ROOT = ..
include ../Make_def

PROG = dial_1 dial_2 
DEMO_C = $(YDIR_ROOT)/demo/demons.c

# =============================================================================
local : $(PROG) 
# =============================================================================
all : 
	@-(cd $(DIR_ROOT) &&  make)
# =============================================================================
dial_1 : $(DEMO_C) 
	$(CC) $(OPTS) -o dial_1 -DD_DIAL_1 $(DEMO_C) $(INCS) $(LIBS)
dial_2 : $(DEMO_C) 
	$(CC) $(OPTS) -o dial_2 -DD_DIAL_2 $(DEMO_C) $(INCS) $(LIBS) 
# =============================================================================
clean :   
	$(RM) $(PROG) *.flc *.o core *~ 
# =============================================================================
   
